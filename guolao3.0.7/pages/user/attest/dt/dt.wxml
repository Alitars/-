				<view class="fl">营业执照</view>
				<view class="fl-mg clearfix" data-id="3">
					<block wx:for="{{arry.business_license}}" wx:key="index">
						<view class="posab">
							<image class="icom-img" src="{{ icon_Url+item }}"></image>
						</view>
					</block>
				</view>
			</view>


