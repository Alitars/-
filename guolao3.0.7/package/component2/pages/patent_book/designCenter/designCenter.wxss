page {
  background:#fff;
  padding-bottom:0rpx;
}
.publish::after{
  border-color: rgba(229, 28, 35, 1);
}
.clearfix:after {
  content:""; 
  display: block; 
  clear:both; 
}
.fl{
  float: left;
}
.fr{
  float :right;
}
.continer {
  position: relative;
  width: 100%;
  height: 100%
}
.order{
  width: 100%;
  height: 100%;
}
.oderinfo{
  border-bottom: 20rpx solid #f5f5f5;
  padding: 20rpx;
  box-sizing: border-box;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.type{
  padding-bottom: 10rpx;
}
.red{
  color: rgba(229, 28, 35, 1);
}
.nameWidth{
  width: 600rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
}
.btn{
  margin-top: 60rpx;
}
.btn button{
  width: 170rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 10rpx;
  font-size: 26rpx;
  text-align: center;
  font-family: Arial;
  /* border: 2rpx solid rgba(229, 28, 35, 1); */
  float: right;
}
.redbtn{
  border-color: rgba(229, 28, 35, 1);
  color: rgba(229, 28, 35, 1);
}
