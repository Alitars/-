page {
  background: #f9f9f9
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.continer {
  padding: 0;
  margin: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  padding-bottom: 70rpx;
  position: relative;
}
.bg{
  height: 224rpx;
  width: 100%;
}
.box{
  border-radius: 24rpx;
  background-color: #fff;
  width: 100%;
  /* height: 100%; */
  position: absolute;
  top: 200rpx;
  padding-left:40rpx;
  padding-right:40rpx;
  box-sizing: border-box;
  
}
.imgbox{
  position: absolute;
  top: 0;
  left:40rpx;
  transform: translateY(-50%);
  width: 200rpx;
  height: 200rpx;
  border-radius: 24rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 2px 4px 12px -2px rgba(211, 211, 211, 0.68);
}
.img{
  width: 170rpx;
  height: 170rpx;
  border-radius: 24rpx;
}
.text{
  position: absolute;
  left:260rpx;
  top: -46rpx;
  color: #fff;
  font-size: 24rpx;
}
.kon{
  width: 100%;
  height: 140rpx;
}
.font,.name,.type{
  font-size: 24rpx;
  margin-bottom:40rpx;
}
.left{
  float: left;
}
.right{
  float: right;
}
.mg-l2{
  margin-left: 20rpx;
}
.w100{
  width:500rpx;
}
.van-cell{
  padding: 0!important;
  border-bottom: 2rpx solid #ccc;
}
.van-cell::after{
  border-bottom: none!important;
}
.van-cell__value {
  text-align: left !important;
  text-indent: 50rpx!important;
  color: #000!important;
}
.tuxin{
  height: 38rpx;
  line-height: 38rpx;
  font-size: 24rpx;
  border-bottom: 2rpx solid #ccc;
  width: 100%;
  text-indent: 50rpx;
}
.comName{
  width: 100%;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 14px;
  border-bottom: 2rpx solid #ccc;
  /* text-indent: 50rpx; */
  padding-left:50rpx;
  box-sizing: border-box;
}


/* c+c */
.title {
  padding: 40rpx 0 40rpx 0;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  /* text-indent: 40rpx */
}

.first-box {
  width: inherit;
  height: 80rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  overflow: hidden;
  position: relative
}

.first-box image {
  position: absolute;
  width: 26rpx;
  height: 22rpx;
  overflow: hidden;
  left: 40rpx;
  -webkit-transform: rotate(-90deg);
  -ms-transform: rotate(-90deg);
  transform: rotate(-90deg)
}

.rotate image {
  transform: rotate(0deg)
}

.first-box text {
  font-size: 28rpx;
  color: #666;
  position: absolute;
  left: 75rpx
}

.flex-box {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  width: inherit;
  height: 100%;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center
}

.center-box-fl {
  width: 600rpx;
  height: inherit;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-direction: row;
  flex-direction: row;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap
}

.flx-btn {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  padding: 10rpx 27rpx 10rpx 27rpx;
  font-size: 24rpx;
  color: #666;
  /* border: solid 1px #f98d4c; */
}

.over {
  overflow: hidden
}

.wx-box {
  width: inherit;
  height: 400rpx;
  padding: 0;
  margin: 0;
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #666
}

.wx-box::after {
  border: none
}

.wx-box image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%
}
