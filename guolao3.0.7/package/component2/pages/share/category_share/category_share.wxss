page {
  background: #f9f9f9
}

.continer {
  padding: 0;
  margin: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  padding-bottom: 70rpx
}

.top-img-box {
  width: inherit;
  height: 336rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  background: #f9f9f9
}

.center-img {
  width: 262rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  height: 262rpx;
  overflow: hidden;
  background: #fff;
  border-radius: 12rpx
}

.center-img image {
  width: 206rpx;
  height: 206rpx;
  overflow: hidden;
  background-color: #f7ede8
}

.title {
  padding: 40rpx 0 40rpx 0;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  text-indent: 40rpx
}

.first-box {
  width: inherit;
  height: 80rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  overflow: hidden;
  position: relative
}

.first-box image {
  position: absolute;
  width: 26rpx;
  height: 22rpx;
  overflow: hidden;
  left: 40rpx;
  -webkit-transform: rotate(-90deg);
  -ms-transform: rotate(-90deg);
  transform: rotate(-90deg)
}

.rotate image {
  transform: rotate(0deg)
}

.first-box text {
  font-size: 28rpx;
  color: #666;
  position: absolute;
  left: 75rpx
}

.flex-box {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  width: inherit;
  height: 100%;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center
}

.center-box-fl {
  width: 600rpx;
  height: inherit;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-direction: row;
  flex-direction: row;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap
}

.flx-btn {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  padding: 10rpx 27rpx 10rpx 27rpx;
  font-size: 24rpx;
  color: #666;
  /* border: solid 1px #f98d4c; */
}

.over {
  overflow: hidden
}

.wx-box {
  width: inherit;
  height: 400rpx;
  padding: 0;
  margin: 0;
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #666
}

.wx-box::after {
  border: none
}

.wx-box image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%
}