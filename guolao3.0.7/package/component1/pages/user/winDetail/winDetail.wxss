page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.continer{
  position: relative;
}
.kong{
  width: 100%;
  height: 20rpx;
}
.padding{
  padding: 24rpx 60rpx;
  padding-top: 0;
}
.box{
  width: 100%;
  border-bottom: 2rpx dashed #979797;
  box-sizing: border-box;
  padding: 24rpx 0;
}
.box view{
  font-family: PingFangSC;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  letter-spacing: normal;
}
.left{
  float: left;
}
.right{
  float: right;
}