page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.bg{
  height: 540rpx;
  width: 100%;
}
.abImg{
  height: 160rpx;
  width: 90%;
  position: absolute;
  top: 440rpx;
  left: 50%;
  transform: translateX(-50%);
}
.continer{
  position: relative;
}
.myNum{
  width: 248rpx;
  height: 248rpx;
  border-radius: 50%;
  /* background:-webkit-linear-gradient(top,#fa7105,#FACF94,white); */
  background-image: linear-gradient(#FCB891,#FB9C64);
  position: absolute;
  top: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.tit{
  padding-top: 70rpx;
}
.tit1{
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  font-family: PingFangSC;
  font-size: 28rpx;
  color: #fff;
}
.tit2{
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  font-family: PingFangSC;
  font-size:46rpx;
  font-weight: 500;
  color: #fff;
}
.numDetail{
  position: absolute;
  width: 180rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: -30rpx;
  background-color: #FEEBDF;
  border-radius: 50rpx;
  padding: 10rpx 10rpx;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  letter-spacing: normal;
  text-align: center;
  color: #fa7105;
}
.rule{
  position: absolute;
  top: 20rpx;
  right: 40rpx;
  font-size: 24rpx;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  letter-spacing: normal;
}
.rule image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.rule text{
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #f3f3f3;
  padding-top:10rpx;
  box-sizing: border-box;
}
.kon{
  width: 100%;
  height: 80rpx;
  background-color: #fff;
}
.taskTit{
  position: relative;
}
.taskTit image{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 0;
  left: 40rpx;
}
.taskTit view{
  padding-left: 100rpx;
  font-family: PingFangSC;
  font-size: 28rpx;
  font-weight: 500;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
}
.ruleDetail{
  padding:40rpx;
  width: 100%;
  box-sizing: border-box;
}
.box{
  position: relative;
}
.rleft{
  padding-top: 20rpx;
  position: relative;
}
.qBtn{
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
  height: 52rpx;
  line-height: 52rpx;
  width: 162rpx;
  text-align: center;
  border-radius: 26rpx;
  border: solid 2rpx #fa7105;
  color: #fa7105;
  font-size: 26rpx;
}
.fl{
  float: left;
}
.name{
  font-family: PingFangSC;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  color: #121314;
}
.tip{
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #fff;
  background-color: #fa7105;
  border-radius: 40rpx 30rpx 30rpx 0;
  width: 116rpx;
  position: absolute;
  bottom: 0;
  left: 200rpx;
}
.rbot{
  padding: 20rpx 0;
  border-bottom: 2rpx solid #F5F5F5;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  color: #999999;
}

/* 积分商城 */
.shop{
  margin-top: 10rpx;
