  text-align: center;
  width: 100%;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  letter-spacing: normal;
  color: #999999;
}
.line{
  position: absolute;
  top: 50%;
  left: 20%;
  height: 2rpx;
  background-color: #999999;
  width: 150rpx;
}
.rline{
  position: absolute;
  top: 50%;
  right: 20%;
  height: 2rpx;
  background-color: #999999;
