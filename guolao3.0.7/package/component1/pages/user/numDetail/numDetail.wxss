page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.continer{
  position: relative;
}
.list-view {
  width: inherit;
  height: 164rpx;
  background: #fff;
  border-bottom: 1px solid #f9f9f9;
  position: relative
}

.img-num {
  width: 84rpx;
  height: 96rpx;
  overflow: hidden;
  position: absolute;
  left: 40rpx;
  top: 50%;
  margin-top: -48rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  overflow: hidden;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center
}

.img-num text {
  font-size: 20px;
  color: #fff;
  position: absolute;
  z-index: 10
}

.list-view-img {
  position: absolute;
  width: inherit;
  height: inherit;
  left: 0;
  top: 0
}
.btn-dt {
  font-size: 24rpx;
  color: #fa7105;
  position: absolute;
  right: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
  right: 40rpx;
  top: 50%;
  margin-top: -50rpx
}

.apply-name {
  position: relative;
  left: 142rpx;
  height: 30%;
  width: 450rpx;
  max-width: 450rpx;
  top: 20%;
  font-size: 28rpx;
  color: #2b3139;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center
}

.apply-name text {
  white-space: nowrap;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis
}

.gray {
  color: #999
}