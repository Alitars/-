    <view class="t_box">申请类别</view>
    <view class="btn_box">
      <block wx:for="{{ applyType }}" wx:key="index">
        <view class="list_btn {{ item.isSelect?'org':'' }}" data-id="{{ index }}" bindtap="onCheck">{{ item.name }}</view>
      </block>
    </view>
    <view class="b_btn" style="padding-bottom:{{ SafeButtom  }}rpx">
      <view class="rest" bindtap="onRest">重置</view>
