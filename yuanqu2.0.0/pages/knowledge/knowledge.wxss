page {
  background: #f9f9f9;
}

.list_box_top {
  width: 100%;
}

.blue {
  background: -webkit-linear-gradient(left, #2fa5fc, #277cfb);
  color: #fff;
}

.body {
  border-top: 1rpx solid #eee;
  width: 100%;
  height: 100%;
  background: #fff;
}

.list_icon_box {
  width: 33%;
  height: 132rpx;
  float: left;
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  color: #333;
  text-align: center;
  background: #fff;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #eee;
}

.icon_d {
  width: 64rpx;
  height: 64rpx;
  margin: 0 auto;
}

.d_t {
  margin-top: 10rpx;
}

.jqqd {
  width: 200rpx;
  margin-top: 200rpx;
}

.qidai {
  font-size: 28rpx;
  margin-top: 40rpx;
  color: #2fa5fc;
}

.navbar-text {
  font-size: 32rpx;
  font-weight: bold;
  /* left: 30rpx; */
  max-width: 400rpx;
  position: relative;
  padding-right: 40rpx;
}

.navbar-text image {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -15rpx;
}

.text {
  color: #2fa5fc;
}

.titile {
  font-size: 32rpx;
  color: 3333;
  font-weight: 500;
  background: #fff;
  text-indent: 30rpx;
  padding-top: 26rpx;
}

.bd-none {
  padding-top: 200rpx;
  border: 0;
}

.success {
  color: #2fa5fc;
}

.van-popup {
  width: 100%;
  position: fixed;
  left: 0;
  height: 1005;
  z-index: 10;
}
.let-arrow{
  width: 45px;
  height: 45px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.let-arrow image{
  width: 34rpx;
  height: 34rpx;
}