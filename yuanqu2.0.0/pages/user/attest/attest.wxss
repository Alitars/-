.a_box {
  width: 600rpx;
  height: 80rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  border-radius: 40rpx;
  position: relative;
  top: 0rpx;
  left: 50%;
  margin-left: -300rpx;
  background: -webkit-linear-gradient(right, #f96006, #fb9a60);
  -webkit-box-shadow: 0 5px 5px rgba(249, 96, 6, 0.3);
  box-shadow: 0 5px 5px rgba(249, 96, 6, 0.3);
  overflow: hidden
}

.a_box:after {
  content: "";
  background: #f96006;
  position: absolute;
  width: 750rpx;
  height: 750rpx;
  left: calc(50% - 375rpx);
  top: calc(50% - 375rpx);
  opacity: 0;
  margin: auto;
  border-radius: 50%;
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
  -webkit-transition: all .4s ease-in-out;
  -o-transition: all .4s ease-in-out;
  transition: all .4s ease-in-out
}

.a_box:active:after {
  -webkit-transform: scale(0);
  -ms-transform: scale(0);
  transform: scale(0);
  opacity: 1;
  -webkit-transition: 0s;
  -o-transition: 0s;
  transition: 0s
}

