      <view class="l-box" data-index="{{ index }}" bindtouchmove="{{ item.status==3||item.status==1?'onTap':'' }}"></view>
      <view class="r-box" data-index="{{ index }}" bindtouchmove="{{ item.status==3||item.status==1?'onTap2':'' }}"></view>
